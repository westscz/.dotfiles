{
    "gitlens.showWhatsNewAfterUpgrades": false,
    "git.enableSmartCommit": true,
    "latex-workshop.view.pdf.viewer": "browser",
    "git.autofetch": true,
    "vsnotes.defaultNotePath": "/home/jarek/Notebook",
    "files.trimTrailingWhitespace": true,
    "hemingway.enabled": false,
    "explorer.confirmDelete": false,
    "[html]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter",
    },
    "window.zoomLevel": 1,
    "polacode.backgroundColor": "#fff",
    "polacode.shadow": "rgba(0, 0, 0, 0.15) 0px 0px 35px",
    "polacode.transparentBackground": true,
    "codesnap.backgroundColor": "#fff",
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.15) 0px 0px 35px",
    "codesnap.transparentBackground": true,
    "codesnap.showWindowControls": false,
    "kite.showWelcomeNotificationOnStartup": false,
    "kite.showGoBetaNotification": false,
    "workbench.startupEditor": "none",
    "workbench.enableExperiments": false,
    "workbench.tree.indent": 8,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "update.showReleaseNotes": false,
    "[markdown]": {
        "editor.fontFamily": "\"IA Writer Duo S\", monospace",
        "editor.fontLigatures": true,
        "editor.fontSize": 16,
        "editor.lineHeight": 26,
        "editor.wordWrap": true,
        "zenMode.centerLayout": true,
        "editor.lineNumbers": "off",
        "editor.quickSuggestions": false,
        "editor.rulers": [],
        "editor.codeLens": false,
        "editor.minimap.enabled": true,
    },
    "hediet.vscode-drawio.local-storage": "",
    "jupyter.sendSelectionToInteractiveWindow": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
}