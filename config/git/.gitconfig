[alias]
	i = init
	s = status
	p = push
	a = add
	c = commit -am
	b = branch

	ra = remote add
	po = push origin
	co = checkout

	pom = push origin master
	info = remote show origin

	reb = rebase origin/master
	up = commit --amend

[user]
	email = "westscz@gmail.com"
	name = Jaroslaw Piszczala
[push]
	default = simple
[include]
	path = ~/.gitconfig.local
[includeIf "gitdir:~/second-brain/"]
	path = .gitconfig-gitlab
